{
  "name": "gatsby-starter-default",
  "description": "Gatsby default starter",
  "version": "1.0.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "chroma-js": "^1.3.4",
    "gatsby": "^1.3.0",
    "gatsby-link": "^1.0.1",
    "gatsby-plugin-catch-links": "^1.0.1",
    "gatsby-plugin-react-helmet": "^1.0.1",
    "gatsby-plugin-sharp": "^1.3.0",
    "gatsby-plugin-styled-components": "^1.0.1",
    "gatsby-source-filesystem": "^1.0.1",
    "gatsby-transformer-sharp": "^1.0.1",
    "hast-util-to-html": "^3.1.0",
    "mdast-util-to-hast": "^2.4.2",
    "prismjs": "^1.6.0",
    "react-prism": "^4.3.0",
    "remark": "^8.0.0",
    "sanitize-html": "^1.14.1",
    "styled-components": "^2.1.1",
    "uigradients": "^0.2.1",
    "unist-util-select": "^1.5.0",
    "word-wrap": "^1.2.3"
  },
  "devDependencies": {
    "gh-pages": "^1.0.0",
    "prettier": "^1.5.3",
    "prettier-eslint-cli": "^4.1.1",
    "surge": "^0.19.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "node --harmony ./node_modules/.bin/gatsby build",
    "cleanup": "rm -rf public/* && git checkout -- public && rm -rf .cache",
    "deploy": "surge --project public",
    "develop": "node --harmony ./node_modules/.bin/gatsby develop",
    "format": "prettier --write gatsby-config.js \"src/**/*.js\""
  }
